<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment1">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="60dp"
            android:layout_height="58dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="50dp"
            android:src="@drawable/ic_action_name"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/editTextNumberSigned"
            app:layout_constraintHorizontal_bias="0.051"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:tint="#3F51B5" />

        <EditText
            android:id="@+id/editTextNumberSigned"
            android:layout_width="0dp"
            android:layout_height="46dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="51dp"
            android:ems="10"
            android:inputType="numberSigned"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="43dp"
            android:layout_height="46dp"
            android:background="#FFFFFF"
            android:src="@drawable/ic_plus"
            android:tint="#3F51B5"
            app:layout_constraintStart_toEndOf="@id/editTextNumberSigned"
            app:layout_constraintTop_toTopOf="@id/editTextNumberSigned"
            tools:layout_editor_absoluteX="342dp"
            tools:layout_editor_absoluteY="28dp" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="43dp"
            android:layout_height="46dp"
            android:background="#FFFFFF"
            android:src="@drawable/ic_minus"
            android:tint="#3F51B5"
            app:layout_constraintEnd_toStartOf="@id/editTextNumberSigned"
            app:layout_constraintTop_toTopOf="@id/editTextNumberSigned"
            tools:layout_editor_absoluteX="84dp"
            tools:layout_editor_absoluteY="28dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="90dp"
            android:layout_height="23dp"
            android:text="Подготовка"
            android:textColor="#3F51B5"
            app:layout_constraintBottom_toTopOf="@id/editTextNumberSigned"
            app:layout_constraintEnd_toEndOf="@id/editTextNumberSigned"
            app:layout_constraintStart_toStartOf="@id/editTextNumberSigned"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="185dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>